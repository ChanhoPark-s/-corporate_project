<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.spring.mapper.SellMapper">

	<insert id="insert">
		insert into sell values(sell_seq.nextval,#{member_no},#{order_no},#{day})
	</insert>
	
		<select id="read" resultType="com.spring.domain.SellVO">
		select * from sell order by no desc
	</select>
	
	<delete id="delete">
	delete sell where no = #{no}
	</delete>

	<update id="update">
	update sell set member_no = #{member_no}, order_no = #{order_no}, day = #{day} where no = #{no}
	</update>

</mapper>